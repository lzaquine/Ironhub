{{#each results}}

<div class="row -mt-72">
  <div class="column -mt-72">
    <div class="card glass mb-72 mt-2 z-10 top-1/2 left-1/2 transform -translate-x-1/2 -translate-y-1/2">
    <div class="avatar justify-center">
  <div class="w-24 rounded-full">
      <img src="{{this.profilepicture}}" alt="{{this.username}}">
        </div>
</div>
<h1>{{this.username}}</h1>
<p>{{this.course}}</p>
<p>{{this.campus}}</p>
<button>
  <a href="/profile/{{this.username}}">Go to Profile</a></button></div>
  </div>
</div>

{{/each}}

<footer class="footer footer-center p-1 bg-base-100 text-base-content">
  <div>
    <p>Developed and Designed by <br><a href="https://github.com/lzaquine" target="_blank">Lucas Zaquine</a> & <a href="https://github.com/pveg" target="_blank">Paulo Torres</a></p>
  </div>
</footer>






















